{
  "name": "example10",
  "type": "module",
  "version": "1.0.0",
  "license": "MIT",
  "module": "dist/index.js",
  "scripts": {
    "start": "yarn build && node --watch --enable-source-maps dist/main.js",
    "start:dev": "run-p watch start",
    "watch": "yarn build --watch",
    "clean": "rm -r dist*",
    "test": "yarn build-test && yarn esm-jest",
    "esm-jest": "yarn node --experimental-vm-modules --no-warnings=ExperimentalWarning --enable-source-maps $(yarn bin jest)",
    "build-test": "tsc -b tsconfig.e2e.json",
    "build": "tsc -b tsconfig.build.json",
    "rebuild": "cd ../.. && yarn clean && cd - && yarn build"
  },
  "imports": {
    "#app/*": "./dist/app/*",
    "#routed/*": "./dist/app/modules/routed/*",
    "#service/*": "./dist/app/modules/service/*"
  },
  "devDependencies": {
    "jest": "^30.0.0",
    "npm-run-all": "*",
    "typescript": "^5.9.2"
  }
}
