{
  "name": "@ditsmod/cors",
  "type": "module",
  "version": "3.0.4-alpha.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/ditsmod/ditsmod.git"
  },
  "description": "Ditsmod module to support CORS and OPTIONS requests",
  "module": "dist/index.js",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "test": "tsc -b tsconfig.unit.json && yarn esm-jest",
    "esm-jest": "yarn node --experimental-vm-modules --no-warnings=ExperimentalWarning --enable-source-maps $(yarn bin jest)",
    "build": "tsc -b tsconfig.build.json",
    "clean": "rm -rf dist*"
  },
  "keywords": [
    "ditsmod",
    "cors",
    "options"
  ],
  "author": "Костя Третяк",
  "license": "MIT",
  "dependencies": {
    "@ts-stack/cookies": "^1.2.2",
    "@ts-stack/cors": "^1.4.2"
  },
  "peerDependencies": {
    "@ditsmod/core": "^3.0.4-alpha.4",
    "@ditsmod/rest": "^3.0.4-alpha.4"
  },
  "devDependencies": {
    "jest": "^30.1.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.2"
  },
  "gitHead": "eaf8f20b4e75e38c7a51fd1c3174b691cea756fa"
}
