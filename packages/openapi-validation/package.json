{
  "name": "@ditsmod/openapi-validation",
  "type": "module",
  "version": "3.0.3-alpha.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/ditsmod/ditsmod.git"
  },
  "description": "Ditsmod module to support simple OpenAPI validation",
  "module": "dist/index.js",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "clean": "rm -rf dist*",
    "test": "tsc -b tsconfig.unit.json && yarn esm-jest",
    "esm-jest": "yarn node --experimental-vm-modules --no-warnings=ExperimentalWarning --enable-source-maps $(yarn bin jest)"
  },
  "keywords": [
    "ditsmod"
  ],
  "author": "Костя Третяк",
  "license": "MIT",
  "dependencies": {
    "@ts-stack/cookies": "^1.2.2",
    "ajv": "^8.17.1"
  },
  "peerDependencies": {
    "@ditsmod/core": "^3.0.3-alpha.4",
    "@ditsmod/i18n": "^3.0.3-alpha.4",
    "@ditsmod/openapi": "^3.0.3-alpha.4",
    "@ditsmod/rest": "^3.0.3-alpha.4"
  },
  "devDependencies": {
    "jest": "^30.1.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.2"
  },
  "gitHead": "eaf8f20b4e75e38c7a51fd1c3174b691cea756fa"
}
