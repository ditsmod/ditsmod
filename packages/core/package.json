{
  "name": "@ditsmod/core",
  "type": "module",
  "version": "3.0.0-alpha.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/ditsmod/ditsmod.git"
  },
  "homepage": "https://ditsmod.github.io/en/",
  "description": "NodeJS web framework writen in TypeScript",
  "module": "dist/index.js",
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "clean": "rm -rf dist*",
    "test": "yarn build-test && yarn esm-jest",
    "build-test": "tsc -b tsconfig.unit.json",
    "esm-jest": "yarn node --experimental-vm-modules --no-warnings=ExperimentalWarning --enable-source-maps $(yarn bin jest)"
  },
  "imports": {
    "#di": "./dist/di.js",
    "#di/*": "./dist/di/*",
    "#init/*": "./dist/init/*",
    "#types/*": "./dist/types/*",
    "#utils/*": "./dist/utils/*",
    "#models/*": "./dist/models/*",
    "#decorators/*": "./dist/decorators/*",
    "#logger/*": "./dist/logger/*",
    "#errors": "./dist/errors.js",
    "#error/*": "./dist/error/*",
    "#services/*": "./dist/services/*",
    "#extension/*": "./dist/extension/*",
    "#interceptors/*": "./dist/interceptors/*"
  },
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./di": {
      "types": "./dist/di.d.ts",
      "default": "./dist/di.js"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "default": "./dist/errors.js"
    },
    "./errors-helpers": {
      "types": "./dist/errors-helpers.d.ts",
      "default": "./dist/errors-helpers.js"
    }
  },
  "keywords": [
    "ditsmod"
  ],
  "author": "Костя Третяк <ktretiak.in.ua@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 20.6.0"
  },
  "dependencies": {
    "@ts-stack/chain-error": "^1.5.1",
    "@ts-stack/stack-utils": "^1.1.0",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "jest": "^30.1.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.2"
  },
  "gitHead": "eaf8f20b4e75e38c7a51fd1c3174b691cea756fa"
}
