{
  "name": "@ditsmod/body-parser",
  "type": "module",
  "version": "3.0.0-alpha.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/ditsmod/ditsmod.git"
  },
  "description": "Body parser module for Ditsmod applications",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "clean": "rm -rf dist*",
    "test": "tsc -b tsconfig.unit.json && yarn esm-jest",
    "esm-jest": "yarn node --experimental-vm-modules --no-warnings=ExperimentalWarning --enable-source-maps $(yarn bin jest)"
  },
  "keywords": [
    "ditsmod"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ts-stack/body-parser": "^1.2.0",
    "@ts-stack/multer": "^1.0.0"
  },
  "peerDependencies": {
    "@ditsmod/core": "^3.0.0-alpha.3",
    "@ditsmod/rest": "^3.0.0-alpha.3"
  },
  "devDependencies": {
    "jest": "^30.1.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.2"
  }
}
