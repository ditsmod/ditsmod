{
  "name": "@ditsmod/authjs",
  "type": "module",
  "version": "3.0.0-alpha.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/ditsmod/ditsmod.git"
  },
  "homepage": "https://ditsmod.github.io/en/",
  "description": "Ditsmod module to support Auth.js",
  "module": "dist/index.js",
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "clean": "rm -rf dist*",
    "test": "yarn build-test && yarn esm-jest",
    "esm-jest": "yarn node --experimental-vm-modules --no-warnings=ExperimentalWarning --enable-source-maps $(yarn bin jest)",
    "build-test": "tsc -b tsconfig.e2e.json"
  },
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./providers": {
      "types": "./dist/providers/index.d.ts"
    },
    "./providers/*": {
      "types": "./dist/providers/*.d.ts",
      "import": "./dist/providers/*.js"
    },
    "./adapters": {
      "types": "./dist/adapters.d.ts"
    }
  },
  "imports": {
    "#mod/*": "./dist/*"
  },
  "keywords": [
    "ditsmod",
    "auth.js",
    "authjs",
    "auth",
    "oauth2",
    "authentication",
    "oidc",
    "csrf"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@ditsmod/body-parser": "^3.0.0-alpha.3",
    "@ditsmod/core": "^3.0.0-alpha.3",
    "@ditsmod/rest": "^3.0.0-alpha.3"
  },
  "dependencies": {
    "@auth/core": "^0.37.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "jest": "^30.0.0",
    "npm-run-all": "*",
    "typescript": "*"
  }
}
