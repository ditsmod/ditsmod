{
  "private": true,
  "name": "@ditsmod/rest-diagnostics",
  "type": "module",
  "version": "3.0.0-alpha.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/ditsmod/ditsmod.git"
  },
  "description": "Ditsmod module to support Node.js diagnostics channel",
  "module": "dist/index.js",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "clean": "rm -rf dist*",
    "test": "tsc -b tsconfig.unit.json && yarn esm-jest",
    "esm-jest": "yarn node --experimental-vm-modules --no-warnings=ExperimentalWarning --enable-source-maps $(yarn bin jest)"
  },
  "keywords": [
    "ditsmod",
    "diagnostics-channel",
    "diagnostics"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@ditsmod/core": "^3.0.0-alpha.3",
    "@ditsmod/rest": "^3.0.0-alpha.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "jest": "^30.1.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.2"
  }
}
