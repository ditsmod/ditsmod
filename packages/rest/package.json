{
  "name": "@ditsmod/rest",
  "type": "module",
  "version": "3.0.0-alpha.5",
  "author": "Костя Третяк <ktretiak.in.ua@gmail.com>",
  "homepage": "https://ditsmod.github.io/en/",
  "description": "Rest module for Ditsmod applications",
  "module": "dist/index.js",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./diagnostics-channel": {
      "default": "./dist/diagnostics-channel.js"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "default": "./dist/errors.js"
    }
  },
  "imports": {
    "#diagnostics-channel": "./dist/diagnostics-channel.js",
    "#errors": "./dist/errors.js",
    "#utils/*": "./dist/utils/*",
    "#init/*": "./dist/init/*",
    "#types/*": "./dist/types/*",
    "#decorators/*": "./dist/decorators/*",
    "#services/*": "./dist/services/*",
    "#providers/*": "./dist/providers/*",
    "#interceptors/*": "./dist/interceptors/*",
    "#extensions/*": "./dist/extensions/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ditsmod/ditsmod.git"
  },
  "scripts": {
    "build": "tsgo -b tsconfig.build.json",
    "clean": "rm -rf dist*",
    "test": "yarn build-test && yarn esm-jest",
    "build-unit": "tsgo -b tsconfig.unit.json",
    "test-unit": "tsgo -b tsconfig.unit.json && yarn esm-jest",
    "esm-jest": "yarn node --experimental-vm-modules --no-warnings=ExperimentalWarning --enable-source-maps $(yarn bin jest)",
    "build-test": "tsgo -b tsconfig.e2e.json",
    "bench": "yarn build && node --enable-source-maps --expose-gc dist/bench.js",
    "rebuild": "cd ../.. && yarn clean && cd - && yarn build"
  },
  "keywords": [
    "ditsmod",
    "router",
    "rest",
    "typescript"
  ],
  "devDependencies": {
    "@typescript/native-preview": "^7.0.0-dev.20260107.1",
    "find-my-way": "^9.3.0",
    "jest": "^30.1.3",
    "koa-tree-router": "^0.13.1",
    "npm-run-all": "^4.1.5",
    "trek-router": "^1.2.0",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "@ditsmod/core": "^3.0.0-alpha.5"
  },
  "gitHead": "eaf8f20b4e75e38c7a51fd1c3174b691cea756fa",
  "publishConfig": {
    "access": "public"
  }
}
